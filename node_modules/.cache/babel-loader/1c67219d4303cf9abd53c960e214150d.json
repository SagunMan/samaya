A,IAAA,EAAA,EAAA,KAAA,GAAA,YAAA,CAAA,OAAA,CAAA,KACA,IAAA,OAAA,GAAA,EAAA,IAAA,OAAA,IAAA,EAAA,EAAA,KAAA,GAAA,YAAA,CAAA,KAAA,CAbA,CAcA;;AACA,aAAA,MAAA,GAAA,GAAA,GAAA,KAAA,GAAA,UAAA;AACA;AAlBA;AAbA,CAAA","sourcesContent":["<template>\n  <div class=\"greeting\">{{greeting}}</div>\n</template>\n\n<script>\nimport { mapGetters } from \"vuex\";\n\nexport default {\n  name: \"Greetings\",\n  props: {},\n  data() {\n    return {};\n  },\n\n  created() {\n    // this.startClock();\n  },\n\n  methods: {},\n\n  computed: {\n    ...mapGetters(\"settings\", [\"clockFormat\"]),\n    greeting() {\n      var prefix = \"शुभ\";\n      var translations = {\n        morning: \"प्रभात\",\n        noon: \"मध्यान्ह\",\n        evening: \"सन्ध्या\",\n        night: \"रात्री\"\n      };\n      let trans;\n      let hourNow = new Date().getHours();\n      if (hourNow >= 1 && hourNow <= 12) trans = translations.morning;\n      else if (hourNow > 12 && hourNow <= 16) trans = translations.noon;\n      else if (hourNow > 16 && hourNow <= 19) trans = translations.evening;\n      else if (hourNow > 19 && hourNow <= 24) trans = translations.night;\n      // debugger;\n      return prefix + \" \" + trans + \", बिभुति\";\n    }\n  }\n};\n</script>\n\n<!-- Add \"scoped\" attribute to limit CSS to this component only -->\n<style lang=\"scss\" scoped>\n.greeting {\n  margin-top: -3rem;\n  font-size: 3rem;\n  color: white;\n}\n</style>\n"],"sourceRoot":"src/components"}]}