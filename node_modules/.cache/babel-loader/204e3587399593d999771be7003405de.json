,EAAA;AAAA,QAAA,IAAA,EAAA;AAAA,OAAA,CAHA,EAIA,CAAA,YAAA,CAJA,EAKA,CAAA;AAAA,QAAA,KAAA,EAAA;AAAA,OAAA,EAAA;AAAA,QAAA,UAAA,EAAA;AAAA,OAAA,CALA;AAZA,KAAA;AAoBA,GAxBA;AA0BA,EAAA,OA1BA,qBA0BA;AACA,SAAA,OAAA;AACA,SAAA,YAAA,GAAA,KAAA,KAAA,CAAA,CAAA,CAAA;AACA,GA7BA;AA+BA,EAAA,OAAA,EAAA;AACA,IAAA,WADA,yBACA;AACA,WAAA,SAAA,GAAA,CAAA,KAAA,SAAA;AACA,KAHA;AAKA,IAAA,UALA,sBAKA,IALA,EAKA;AACA,WAAA,YAAA,GAAA,IAAA;AACA,KAPA;AASA,IAAA,OATA,qBASA;AACA,UAAA,KAAA,KAAA,CAAA,MAAA,IAAA,EAAA,EAAA,KAAA,CAAA,uBAAA,CAAA;AACA,UAAA,GAAA,GAAA,IAAA,CAAA,GAAA,EAAA;AACA,WAAA,KAAA,CAAA,IAAA,CAAA;AACA,QAAA,EAAA,EAAA,IAAA,EADA;AAEA,QAAA,KAAA,EAAA,eAAA,KAAA,KAAA,CAAA,MAAA,GAAA,CAAA,CAFA;AAGA,QAAA,IAAA,EAAA,SAAA,WAAA,IAAA,KAAA,KAAA,CAAA,MAAA,GAAA,CAAA,IAAA,OAHA;AAIA,QAAA,SAAA,EAAA,GAJA;AAKA,QAAA,UAAA,EAAA;AALA,OAAA;AAOA,KAnBA;AAqBA,IAAA,UArBA,sBAqBA,EArBA,EAqBA;AACA,UAAA,KAAA,GAAA,KAAA,KAAA,CAAA,SAAA,CAAA,UAAA,IAAA;AAAA,eAAA,IAAA,CAAA,EAAA,KAAA,EAAA;AAAA,OAAA,CAAA;AACA,WAAA,KAAA,CAAA,MAAA,CAAA,KAAA,EAAA,CAAA;AACA,KAxBA;AA0BA,IAAA,gBA1BA,8BA0BA;AACA,UAAA,MAAA,GAAA,IAAA,SAAA,EAAA;AACA,UAAA,GAAA,GAAA,MAAA,CAAA,eAAA,CACA,KAAA,YAAA,CAAA,IAAA,CAAA,KAAA,CAAA,CAAA,EAAA,GAAA,CADA,EAEA,WAFA,CAAA;AAIA,UAAA,GAAA,CAAA,IAAA,CAAA,QAAA,CAAA,MAAA,EACA,KAAA,YAAA,CAAA,KAAA,GAAA,GAAA,CAAA,IAAA,CAAA,QAAA,CAAA,CAAA,EAAA,WAAA;AACA;AAlCA,GA/BA;AAoEA,EAAA,QAAA,kCACA,UAAA,CAAA,UAAA,EAAA,CAAA,aAAA,CAAA,CADA;AAGA,IAAA,aAHA,2BAGA;AACA,aAAA,MAAA,CAAA,KAAA,YAAA,CAAA,SAAA,EAAA,GAAA,CAAA,CAAA,OAAA,EAAA;AACA;AALA;AApEA,CAAA","sourcesContent":["<template>\n  <div class=\"notepad\">\n    <div class=\"toggle\" :class=\"{'--active': showNotes}\" @click=\"toggleNotes()\">\n      <i class=\"fal fa-clipboard-list-check\"></i>\n      <span>Notes</span>\n    </div>\n\n    <div class=\"notepadContainer\" v-if=\"showNotes\">\n      <!-- <vue-custom-scrollbar> -->\n\n      <div class=\"notepadNav\">\n        <div class=\"notepadNavHeader\" @click=\"addNote()\">\n          <i class=\"fal fa-plus\"></i>\n          New Note\n        </div>\n        <vue-custom-scrollbar>\n          <div class=\"notepadNavBody\">\n            <div\n              class=\"notepadNavItem\"\n              :class=\"{'--active': note.id === selectedNote.id}\"\n              v-for=\"note in notes\"\n              :key=\"note.id\"\n              @click=\"selectNote(note)\"\n            >\n              <span>{{note.title || 'titleless ðŸ˜’'}}</span>\n              <div class=\"btnTrash\" @click=\"deleteNote(note.id)\">\n                <i class=\"fal fa-times\"></i>\n              </div>\n            </div>\n          </div>\n        </vue-custom-scrollbar>\n      </div>\n\n      <div class=\"notepadEditor\">\n        <div class=\"metadata\">\n          <i class=\"fal fa-clock\"></i>\n          <span>{{noteWrittenAt}}</span>\n        </div>\n        <vue-editor\n          v-model=\"selectedNote.text\"\n          :editorToolbar=\"customToolbar\"\n          @text-change=\"computeNoteTitle()\"\n        ></vue-editor>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport { mapGetters } from \"vuex\";\nimport { v1 as uuid } from \"uuid\";\nimport { VueEditor } from \"vue2-editor\";\nimport moment from \"moment\";\nimport vueCustomScrollbar from \"vue-custom-scrollbar\";\n\nexport default {\n  name: \"NotePad\",\n  components: { VueEditor, vueCustomScrollbar },\n  data() {\n    return {\n      showNotes: false,\n      notes: [\n        {\n          id: \"123123-123123-123213-123\",\n          title: \"Test note longer title\",\n          text: \"This is a test note\",\n          createdAt: 1598095559688,\n          upadatedAt: 1598095559688\n        }\n      ],\n      selectedNote: null,\n      customToolbar: [\n        [{ header: [1, 2, 3, 4, 5, 6, false] }],\n        [\"bold\", \"italic\", \"underline\", \"strike\"],\n        [{ list: \"ordered\" }, { list: \"bullet\" }],\n        [\"code-block\"],\n        [{ color: [] }, { background: [] }]\n      ]\n    };\n  },\n\n  created() {\n    this.addNote();\n    this.selectedNote = this.notes[0];\n  },\n\n  methods: {\n    toggleNotes() {\n      this.showNotes = !this.showNotes;\n    },\n\n    selectNote(note) {\n      this.selectedNote = note;\n    },\n\n    addNote() {\n      if (this.notes.length >= 20) alert(\"Ohhh..that's too much\");\n      let now = Date.now();\n      this.notes.push({\n        id: uuid(),\n        title: \"New note \" + (this.notes.length + 1),\n        text: \"<h1>\" + \"New note \" + (this.notes.length + 1) + \"</h1>\",\n        createdAt: now,\n        upadatedAt: now\n      });\n    },\n\n    deleteNote(id) {\n      let index = this.notes.findIndex(note => note.id === id);\n      this.notes.splice(index, 1);\n    },\n\n    computeNoteTitle() {\n      let parser = new DOMParser();\n      let doc = parser.parseFromString(\n        this.selectedNote.text.slice(0, 100),\n        \"text/html\"\n      );\n      if (doc.body.children.length)\n        this.selectedNote.title = doc.body.children[0].textContent;\n    }\n  },\n\n  computed: {\n    ...mapGetters(\"settings\", [\"clockFormat\"]),\n\n    noteWrittenAt() {\n      return moment(this.selectedNote.createdAt, \"x\").fromNow();\n    }\n  }\n};\n</script>\n\n<!-- Add \"scoped\" attribute to limit CSS to this component only -->\n<style lang=\"scss\" scoped>\n.toggle {\n  display: flex;\n  flex-direction: column;\n  padding: 0.25rem;\n  color: white;\n  border-radius: 3px;\n  cursor: pointer;\n  text-align: center;\n  @include transition;\n  @include not-selectable;\n\n  i {\n    font-size: 1.2rem;\n  }\n\n  span {\n    font-size: 0.8rem;\n  }\n\n  &:hover {\n    background-color: rgba($color: white, $alpha: 0.1);\n  }\n\n  &.--active {\n    background-color: rgba($color: white, $alpha: 0.1);\n  }\n}\n\n.notepadContainer {\n  $background: $white-background;\n\n  display: flex;\n  position: absolute;\n  left: calc(100% + 10px);\n  top: 0;\n  height: 300px;\n  width: 600px;\n  padding: 0.25rem;\n  background-color: $background;\n  color: #373737;\n  border-radius: 5px;\n\n  &:after {\n    content: \"\";\n    position: absolute;\n    left: 0;\n    top: 10px;\n    width: 0;\n    height: 0;\n    border: 10px solid transparent;\n    border-right-color: $background;\n    border-left: 0;\n    border-bottom: 0;\n    margin-top: -5px;\n    margin-left: -10px;\n  }\n\n  .notepadNav {\n    display: flex;\n    flex-direction: column;\n    border-top-left-radius: 5px;\n    border-bottom-left-radius: 5px;\n\n    .notepadNavHeader {\n      width: 100px;\n      padding: 0.25rem 0.5rem;\n      text-align: left;\n      font-size: 0.8rem;\n      cursor: pointer;\n      color: black;\n      @include not-selectable;\n\n      i {\n        margin-right: 5px;\n      }\n\n      &:hover {\n        color: $blue;\n      }\n    }\n\n    .notepadNavItem {\n      display: flex;\n      justify-content: space-between;\n      width: 100px;\n      padding: 0.25rem 0.5rem;\n      padding-right: 0.25rem;\n      text-align: left;\n      font-size: 0.8rem;\n      cursor: default;\n      @include transition;\n      @include not-selectable;\n\n      &.--active {\n        background-color: rgba(white, 0.5);\n\n        .btnTrash {\n          opacity: 1;\n        }\n      }\n\n      &:hover .btnTrash {\n        opacity: 1;\n      }\n\n      span {\n        flex: 1;\n        max-width: 90px;\n        align-self: center;\n        white-space: nowrap;\n        overflow: hidden;\n        text-overflow: ellipsis;\n      }\n\n      .btnTrash {\n        $size: 1rem;\n        align-self: center;\n        width: $size;\n        height: $size;\n        line-height: $size;\n        margin-left: 5px;\n        font-size: 0.7rem;\n        text-align: center;\n        opacity: 0;\n        cursor: pointer;\n        @include transition;\n\n        &:hover {\n          color: $blue;\n        }\n      }\n    }\n  }\n\n  .notepadEditor {\n    position: relative;\n    flex: 1;\n    background-color: rgba(white, 0.5);\n    height: 300px;\n\n    // Customize Quill Editor\n    ::v-deep .ql-toolbar {\n      border: none;\n      padding: 0;\n      padding-right: 0;\n      text-align: right;\n\n      .ql-formats {\n        margin-right: 0;\n        margin-bottom: 5px;\n\n        .ql-header {\n          span[role=\"button\"] {\n            outline: none;\n          }\n          svg {\n            display: none;\n          }\n        }\n      }\n\n      button,\n      span[role=\"button\"] {\n        svg {\n          width: 1rem !important;\n          height: 1rem !important;\n        }\n      }\n    }\n    ::v-deep #quill-container {\n      border: none;\n\n      .ql-editor {\n        padding: 0.25rem;\n        font-size: 0.8rem;\n        overflow: auto;\n        height: calc(300px - 30px);\n        width: calc(600px - 120px);\n      }\n    }\n\n    .metadata {\n      display: flex;\n      position: absolute;\n      top: 3px;\n      left: 0;\n      font-size: 0.8rem;\n      padding-left: 0.25rem;\n      color: gray;\n\n      i {\n        align-self: center;\n        margin-right: 5px;\n      }\n\n      span {\n        align-self: center;\n      }\n    }\n  }\n}\n</style>\n"],"sourceRoot":"src/components"}]}